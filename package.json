{
	"name": "cs-bagrut-helpers",
	"displayName": "C# Bagrut Helpers",
	"description": "Includes generators to speed up work on Computer Science projects",
	"version": "0.0.1",
	"publisher": "noamalffasy",
	"engines": {
		"vscode": "^1.36.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.generatePropertyFunctions"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.generatePropertyFunctions",
				"title": "Generate Property Functions"
			}
		]
