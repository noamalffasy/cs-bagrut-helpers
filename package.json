		"test": "yarn compile && node ./out/test/runTest"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.2"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/noamalffasy/cs-bagrut-helpers"
	},
	"license": "MIT"
}
